t={Edit} />\n      <Route path=\"/create\">\n        <Create />\n      </Route>\n      \n        <h1>Backpack App</h1>\n        <br />\n        <br />\n        <h2>Organize your Backpack for your next Hike</h2>\n        <br />\n        <br />\n        <div>\n          <TextField\n            id=\"outlined-basic\"\n            label=\"Item name\"\n            variant=\"outlined\"\n            color=\"secondary\"\n            placeholder=\"Please enter Item name\"\n            sx={{ bgcolor: \"white\" }}\n            type=\"text\"\n            value={name}\n            onChange={handleChange}\n            style={{ margin: \"20px\" }}\n          />\n          <TextField\n            id=\"outlined-basic\"\n            label=\"Item Weight in gramms\"\n            variant=\"outlined\"\n            color=\"secondary\"\n            placeholder=\"Eenter Item weight in g\"\n            sx={{ bgcolor: \"white\" }}\n            value={weight}\n            onChange={handleWeightChange}\n            style={{ margin: \"20px\" }}\n          />\n          <Button\n            variant=\"contained\"\n            type=\"button\"\n            onClick={handleAdd}\n            style={{ margin: \"30px\" }}\n          >\n            Add to the Backpack!\n          </Button>\n        </div>\n        <br />\n        <br />\n        <Card variant=\"outlined\">\n          <ul style={{ listStyleType: \"none\" }}>\n            {list.map((item) => (\n              <li key={item.id}>\n                {item.name} {item.weight} g\n                <br />\n                <Button\n                  variant=\"contained\"\n                  type=\"button\"\n                  onClick={() => handleRemove(item.id)}\n                >\n                  Remove\n                </Button>\n              </li>\n            ))}\n          </ul>\n        </Card>\n\n        <h2>Total weight: {finalWeightTotal / 1000} kg</h2>\n        <Typography component=\"legend\">Controlled</Typography>\n        <Rating\n          name=\"simple-controlled\"\n          value={value}\n          onChange={(event, newValue) => {\n            setValue(newValue);\n          }}\n        />\n        <Typography component=\"legend\">Read only</Typography>\n        <Rating name=\"read-only\" value={value} readOnly />\n        <Typography component=\"legend\">Disabled</Typography>\n        <Rating name=\"disabled\" value={value} disabled />\n        <Typography component=\"legend\">No rating given</Typography>\n        <Rating name=\"no-value\" value={null} />\n\n        \n      </header>\n    </div>\n  );\n}\n\nexport default App;\n","/home/sier0212/Schreibtisch/WEB DEV Projects/backpack_app/client/src/reportWebVitals.js",[],"/home/sier0212/Schreibtisch/WEB DEV Projects/backpack_app/client/src/components/edit.js",[],"/home/sier0212/Schreibtisch/WEB DEV Projects/backpack_app/client/src/components/navbar.js",[],"/home/sier0212/Schreibtisch/WEB DEV Projects/backpack_app/client/src/components/recordList.js",[],"/home/sier0212/Schreibtisch/WEB DEV Projects/backpack_app/client/src/components/create.js",[],{"ruleId":"34","severity":1,"message":"35","line":32,"column":10,"nodeType":"36","messageId":"37","endLine":32,"endColumn":15},{"ruleId":"34","severity":1,"message":"38","line":32,"column":17,"nodeType":"36","messageId":"37","endLine":32,"endColumn":25},{"ruleId":"34","severity":1,"message":"39","line":36,"column":9,"nodeType":"36","messageId":"37","endLine":36,"endColumn":20},"no-unused-vars","'total' is assigned a value but never used.","Identifier","unusedVar","'setTotal' is assigned a value but never used.","'weightTotal' is assigned a value but never used."]