   console.log(newList);\n\n    setList(newList);\n\n    /* to do:  \n        in a first step: create a component where I store different backpacks, list of backpacks\n\n        secons step: create a global backpack state with redux or context for example and use it in schema? \n        So I can store different backpacks and have acces to it with CRUD functionality\n        need to habe global acces to newList Hook */\n\n    setName(\"\");\n\n    console.log(initialList);\n  }\n\n  function handleRemove(id) {\n    const newList = list.filter((item) => item.id !== id);\n\n    setList(newList);\n    console.log(newList);\n  }\n\n  function handleSortListHeaviestFirst() {\n    const newList = [...list].sort((a, b) => b.weight - a.weight);\n    setList(newList);\n  }\n\n  function handleSortListLightestFirst() {\n    const newList = [...list].sort((a, b) => a.weight - b.weight);\n    setList(newList);\n  }\n\n  return (\n    <div class=\"flexContainer\">\n      <h1 id=\"backpackTitle\">Backpack App</h1>\n      <br />\n      <br />\n      <h2 id=\"backpackDescription\">\n        Organize your Backpack for your next Hike\n      </h2>\n      <br />\n      <br />\n      <div>\n        <TextField\n          id=\"outlined-basic\"\n          label=\"Item name\"\n          variant=\"outlined\"\n          color=\"secondary\"\n          placeholder=\"Please enter Item name\"\n          sx={{ bgcolor: \"white\" }}\n          type=\"text\"\n          value={name}\n          onChange={handleChange}\n          style={{ margin: \"20px\" }}\n        />\n        <TextField\n          id=\"outlined-basic\"\n          label=\"Item Weight in gramms\"\n          variant=\"outlined\"\n          color=\"secondary\"\n          placeholder=\"Enter Item weight in g\"\n          sx={{ bgcolor: \"white\" }}\n          value={weight}\n          onChange={handleWeightChange}\n          style={{ margin: \"20px\" }}\n        />\n        <Button\n          variant=\"contained\"\n          type=\"button\"\n          onClick={handleAdd}\n          style={{ margin: \"30px\" }}\n        >\n          Add to the Backpack!\n        </Button>\n      </div>\n      <br />\n      <br />\n      <div\n        style={{ display: \"flex\", justifyContent: \"center\", width: \"100vw\" }}\n      >\n        <Card variant=\"outlined\">\n          <IconButton\n            aria-label=\"delete\"\n            size=\"small\"\n            onClick={() => handleSortListHeaviestFirst()}\n          >\n            <SortIcon fontSize=\"small\" />\n\n            <h6>heaviest first</h6>\n          </IconButton>\n          <IconButton\n            aria-label=\"delete\"\n            size=\"small\"\n            onClick={() => handleSortListLightestFirst()}\n          >\n            <SortIcon fontSize=\"small\" />\n            <h6>lightest first</h6>\n          </IconButton>\n\n          <ul style={{ listStyleType: \"none\" }}>\n            {list.map((item) => (\n              <li key={item.id}>\n                {item.name} {item.weight} g\n                <IconButton\n                  aria-label=\"delete\"\n                  size=\"small\"\n                  onClick={() => handleRemove(item.id)}\n                >\n                  <DeleteIcon fontSize=\"small\" />\n                </IconButton>\n              </li>\n            ))}\n          </ul>\n        </Card>\n      </div>\n      <Card variant=\"outlined\">\n        <h2 style={{ display: \"flex\", justifyContent: \"center\" }}>\n          <ScaleIcon></ScaleIcon>\n          Total weight: {finalWeightTotal / 1000} kg\n        </h2>\n      </Card>\n    </div>\n  );\n}\n\nexport default Backpack;\n","/home/sier0212/Schreibtisch/WEB DEV Projects/backpack_app/client/src/components/auth/Register.js",[],"/home/sier0212/Schreibtisch/WEB DEV Projects/backpack_app/client/src/components/auth/Login.js",[],"/home/sier0212/Schreibtisch/WEB DEV Projects/backpack_app/client/src/context/AuthContext.js",[],"/home/sier0212/Schreibtisch/WEB DEV Projects/backpac